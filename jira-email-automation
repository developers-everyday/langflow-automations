                "value": "# Required imports for Langflow Custom Component\nfrom langflow.custom import Component\nfrom langflow.io import MessageTextInput, Output, IntInput\nfrom langflow.schema import Data\nimport logging\nfrom datetime import datetime\nimport json\n\n# Configure logging\nlogging.basicConfig(level=logging.INFO, format='%(asctime)s - %(name)s - %(levelname)s - %(message)s')\nlogger = logging.getLogger('JiraDataFetcher')\n\nclass JiraDataFetcher(Component):\n    display_name = \"Jira Data Fetcher\"\n    description = \"Fetches Jira issues from a filter and exports to CSV format\"\n    documentation: str = \"https://docs.langflow.org/components-custom-components\"\n    icon = \"download\"\n    name = \"JiraDataFetcher\"\n    \n    inputs = [\n         MessageTextInput(\n            name=\"request_type\",\n            display_name=\"Request Type\",\n            info=\"Type of Jira report needed (e.g., 'daily_report', 'status_summary', 'brief_update')\",\n            value=\"daily_report\",\n            tool_mode=True,  # This makes it available as a tool for agents\n        ),\n        MessageTextInput(\n            name=\"jira_url\",\n            display_name=\"Jira URL\",\n            info=\"Your Jira instance URL\",\n            value=\"\",\n        ),\n        MessageTextInput(\n            name=\"username\",\n            display_name=\"Username\",\n            info=\"Your Jira username/email\",\n            value=\"\",\n        ),\n        MessageTextInput(\n            name=\"api_token\",\n            display_name=\"API Token\",\n            info=\"Your Jira API token (will be visible in the field)\",\n            value=\"\",\n        ),\n        MessageTextInput(\n            name=\"filter_id\",\n            display_name=\"Filter ID\",\n            info=\"Jira filter ID to export\",\n            value=\"\",\n        ),\n        IntInput(\n            name=\"max_results\",\n            display_name=\"Max Results\",\n            info=\"Maximum number of issues to fetch\",\n            value=1000,\n        ),\n    ]\n    \n    outputs = [\n        Output(display_name=\"Jira Data\", name=\"jira_data\", method=\"fetch_jira_data\"),\n    ]\n    \n    def fetch_jira_data(self) -> Data:\n        \"\"\"Fetch Jira issues and return structured data\"\"\"\n        \n        execution_id = datetime.now().strftime('%Y%m%d_%H%M%S_%f')\n        logger.info(f\"[{execution_id}] Starting Jira data fetch execution\")\n        \n        try:\n            # Import dependencies inside the method to ensure they're available\n            import requests\n            from requests.auth import HTTPBasicAuth\n            import pandas as pd\n            \n            logger.info(f\"[{execution_id}] âœ… All required dependencies imported successfully\")\n            \n            # Access input values using self.{input_name}\n            jira_url = self.jira_url\n            username = self.username\n            api_token = self.api_token\n            filter_id = self.filter_id\n            max_results = self.max_results\n            \n            # Validate inputs\n            if not all([jira_url, username, api_token, filter_id]):\n                error_msg = \"Missing required input parameters. Please provide all fields.\"\n                logger.error(f\"[{execution_id}] âŒ Input validation failed: {error_msg}\")\n                return Data(\n                    value={\"success\": False, \"error\": error_msg, \"error_type\": \"validation\"}\n                )\n            \n            # Log input parameters (mask sensitive data)\n            logger.info(f\"[{execution_id}] Input Parameters:\")\n            logger.info(f\"[{execution_id}]   - Jira URL: {jira_url}\")\n            logger.info(f\"[{execution_id}]   - Username: {username}\")\n            logger.info(f\"[{execution_id}]   - API Token: {'*' * len(api_token) if api_token else 'Not provided'}\")\n            logger.info(f\"[{execution_id}]   - Filter ID: {filter_id}\")