le': 'SAP-Hybris',\n                    'Ticket#': issue['key'],\n                    'Description': issue['fields']['summary'],\n                    'Type': issue['fields']['issuetype']['name'],\n                    'Story Points': issue['fields'].get('customfield_10004', ''),\n                    'Status': issue['fields']['status']['name'],\n                    'Reopen': 'NO',\n                    'Created': issue['fields'].get('created', ''),\n                    'Updated': issue['fields'].get('updated', '')\n                }\n                data_list.append(issue_data)\n                \n                # Log progress every 50 issues\n                if (i + 1) % 50 == 0:\n                    logger.info(f\"[{execution_id}] Processed {i + 1}/{len(issues)} issues\")\n            \n            # Step 5: Create DataFrame and save CSV\n            logger.info(f\"[{execution_id}] Step 4: Creating CSV export\")\n            df = pd.DataFrame(data_list)\n            timestamp = datetime.now().strftime('%Y%m%d_%H%M%S')\n            filename = f\"jira_export_{timestamp}.csv\"\n            df.to_csv(filename, index=False)\n            \n            logger.info(f\"[{execution_id}] CSV saved: {filename}\")\n            \n            # Step 6: Prepare summary statistics\n            logger.info(f\"[{execution_id}] Step 5: Calculating summary statistics\")\n            status_counts = df['Status'].value_counts().to_dict()\n            type_counts = df['Type'].value_counts().to_dict()\n            \n            # Calculate story points (handle empty/null values)\n            story_points_list = []\n            for sp in df['Story Points']:\n                if sp and str(sp).replace('.', '').isdigit():\n                    story_points_list.append(float(sp))\n            \n            total_story_points = sum(story_points_list)\n            \n            # Log summary statistics\n            logger.info(f\"[{execution_id}] Summary Statistics:\")\n            logger.info(f\"[{execution_id}]   - Total Issues: {len(data_list)}\")\n            logger.info(f\"[{execution_id}]   - Total Story Points: {total_story_points}\")\n            logger.info(f\"[{execution_id}]   - Status Distribution: {status_counts}\")\n            logger.info(f\"[{execution_id}]   - Type Distribution: {type_counts}\")\n            \n            # Step 7: Create result data (SIMPLIFIED TO AVOID CIRCULAR REFERENCES)\n            result_summary = {\n                \"success\": True,\n                \"execution_id\": execution_id,\n                \"file_path\": filename,\n                \"total_issues\": len(data_list),\n                \"total_found\": total_found,\n                \"status_summary\": status_counts,\n                \"type_summary\": type_counts,\n                \"total_story_points\": total_story_points,\n                \"export_timestamp\": timestamp,\n                \"summary_text\": f\"Successfully exported {len(data_list)} Jira issues from SAP-Hybris module\",\n                \"jql_query\": jql_query\n            }\n            \n            # Test JSON serialization before creating Data object\n            try:\n                json.dumps(result_summary)\n                logger.info(f\"[{execution_id}] âœ… Result data successfully serialized to JSON\")\n            except (TypeError, ValueError) as json_error:\n                logger.error(f\"[{execution_id}] âŒ JSON serialization failed: {json_error}\")\n                # Create a minimal safe result\n                result_summary = {\n                    \"success\": True,\n                    \"execution_id\": execution_id,\n                    \"total_issues\": len(data_list),\n                    \"file_path\": filename,\n                    \"summary_text\": f\"Successfully exported {len(data_list)} Jira issues\"\n                }\n            \n            logger.info(f\"[{execution_id}] âœ… Execution completed successfully\")\n            logger.info(f\"[{execution_id}] Final Re